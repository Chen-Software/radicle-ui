{
  "redirects": [
    {
      "source": "/rad([^/]+)/i/(.+)",
      "destination": "/rad$1/issues/$2"
    },
    {
      "source": "/rad([^/]+)/p/(.+)",
      "destination": "/rad$1/patches/$2"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
